{
  "name": "retail-order-submit-stack-suite",
  "version": "1.0.0",
  "description": "A TypeScript project for orchestrating API test calls for the Order Submit APIs.",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r dotenv/config src/orchestrator.ts",
    "start:dev": "ts-node -r dotenv/config src/orchestrator.ts dotenv_config_path=.env.dev",
    "start:staging": "ts-node -r dotenv/config src/orchestrator.ts dotenv_config_path=.env.staging",
    "start:prod": "ts-node -r dotenv/config src/orchestrator.ts dotenv_config_path=.env.prod",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
