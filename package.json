    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/inquirer": "^8.2.12",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^17.2.3",
    "eslint": "^8.50.0",
